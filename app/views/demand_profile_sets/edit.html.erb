<% content_for :config_sidebar do %>
  <%= render :partial => 'configurations/config_sidebar', :locals => {:highlighted_item => @dpset} %>
<% end %>

<h2><%= link_to l(:field_demand_profile_set) + "s", project_configuration_demand_profile_sets_path(@project),:class => 'config-header'  %> &#187; <%= l(:demand_profiles_edit) %></h2>


<% labelled_tabular_form_for :demand_profile_set, @dpset, :url => {:action => "update" }, :html => { :method => :put } do |f| %>
	<%= render :partial => 'form', :locals => { :f => f } %>
	<p>
		<%= submit_tag l(:button_update) %>
	</p>
<% end %>

<%= render :partial => 'layouts/scenario_list', :locals => {:record =>  @dpset}  %>

