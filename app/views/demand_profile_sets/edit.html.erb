<% content_for :config_sidebar do %>
  <%= render :partial => 'configurations/config_sidebar', :locals => {:highlighted_item => @dpset} %>
<% end %>

<h2><%= link_to "Demand Profile Sets", :controller => 'demand_profile_sets', :action => 'index' %> &#187; <%= "Edit Demand Profile Set"%></h2>

<% labelled_tabular_form_for :demand_profile_set, @dpset, :url => {:action => "update" }, :html => { :method => :put } do |f| %>
	<%= render :partial => 'form', :locals => { :f => f } %>
	<p>
		<%= submit_tag l(:button_update) %>
	</p>
<% end %>

<%= render :partial => 'layouts/scenario_list' %>
