!!!
%html
  %head
    %title Interactive Report Viewer: #{@report.name}
    :javascript
      var report_url = "#{@report.load_xml_url}";
    = stylesheet_link_tag 'jquery-ui-1.8.15.custom'
    = stylesheet_link_tag 'interactive_report'
    = javascript_include_tag 'jquery-1.6.2.min'
    = javascript_include_tag 'jquery-ui-1.8.15.custom.min'
    = javascript_include_tag 'raphael'
    = javascript_include_tag 'flot/jquery.flot'
    = javascript_include_tag 'interactive_report/base'
    = javascript_include_tag 'interactive_report/data'
    = javascript_include_tag 'interactive_report/ui'
    %script{:type => "text/javascript",
            :src => "http://maps.googleapis.com/maps/api/js?sensor=false"}
  %body
    #debug-log
      %h2
        Log - 
        Crosshairs: (
        %span#crosshair_x 0
        ,
        %span#crosshair_y 0
        )
      %button#log-sliders Log Slider Values
      %br
      %h2
        = @report.name
        \-
        %span#contour_plot_name
    #map-view
    #three-d-graph-container
      #y-axis-slider-container
        #y-axis-slider
      %span#three-d-graph
      #x-axis-slider-container
        #x-axis-slider
    #two-d-graphs
      #xz-chart
      #yz-chart
