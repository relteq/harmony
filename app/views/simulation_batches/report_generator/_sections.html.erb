<div id="sections-time">
	<p>
		<label class="underline"><%=l(:label_time_range)%> </label>
	</p>
	<p>
		<label class="sub-label"><%=l(:label_from)%>: </label><%= f.text_field :b_time,:value => @simulation_report.display_time(:b_time) %>
	</p>
	<p>
	 	<label class="sub-label"><%=l(:label_to)%>: </label> <%= f.text_field :duration,:value => @simulation_report.display_time(:duration) %>
	</p>
</div>
<div id="sections-content">
	<p>
		<label><%=l(:label_contents)%>: </label>
	</p>
	<table class="index list"> 
		<thead>
		  <tr>
				<th>&nbsp;</th>
				<th class="">over time</th>
				<th>over space</th>
				<th>contour</th>
		  </tr>
		</thead>
		<tbody>
			<tr class="user active odd">
		   	<td>Each Link State</td>
		 		<td><%= f.check_box :link_perf %></td>
			  <td>&nbsp;</td>
			  <td>&nbsp;</td>
		  </tr>
			<tr class="user active even">
		   	<td>System Performance</td>
		 		<td><%= f.check_box :network_perf %></td>
			  <td>&nbsp;</td>
			  <td>&nbsp;</td>
		  </tr>
			<tr class="user active odd">
		   	<td>Onramp Performance</td>
		 		<td><%= f.check_box :or_perf_t %></td>
			  <td><%= f.check_box :or_perf_s %></td>
			  <td><%= f.check_box :or_perf_c %></td>
		  </tr>
			<tr class="user active even">
		   	<td>Route Performance</td>
		 		<td><%= f.check_box :route_perf_t %></td>
			  <td><%= f.check_box :route_perf_s %></td>
			  <td><%= f.check_box :route_perf_c %></td>
		  </tr>
			<tr class="user active odd">
		   	<td>Route Travel Time</td>
		 		<td><%= f.check_box :route_tt_t %></td>
		    <td>&nbsp;</td>
			  <td><%= f.check_box :route_tt_c %></td>
		  </tr>
		</tbody>
	</table>
</div>
  <%= masked_input_js 'simulation_batch_report_b_time' %>
  <%= masked_input_js 'simulation_batch_report_duration' %>

