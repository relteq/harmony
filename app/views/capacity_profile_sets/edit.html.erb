<% content_for :config_sidebar do %>
  <%= render :partial => 'configurations/config_sidebar', :locals => {:highlighted_item => @cpset} %>
<% end %>

<h2><%= link_to l(:field_capacity_profile_set) + "s", project_configuration_capacity_profile_sets_path(@project)  %> &#187;  <%= l(:capacity_profiles_edit) %></h2>

<% labelled_tabular_form_for :capacity_profile_set, @cpset, :url => {:action => "update"} , :html => { :method => :put } do |f| %>
	<%= render :partial => 'form', :locals => { :f => f } %>
	<p>
		<%= submit_tag l(:button_update) %>
	</p>
<% end %>

<%= render :partial => 'layouts/scenario_list', :locals => {:record =>  @cpset}  %>

