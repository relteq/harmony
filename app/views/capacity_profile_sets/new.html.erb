<% content_for :config_sidebar do %>
  <%= render :partial => 'configurations/config_sidebar' %>
<% end %>

<h2><%= link_to "Capacity Profile Sets", :controller => 'capacity_profile_sets', :action => 'index' %> &#187; <%= "New Capacity Profile Set"%></h2>

<% labelled_tabular_form_for :capacity_profile_set, @cpset, :url => { :action => "create" }, :html => { :method => :put } do |f| %>
	<%= render :partial => 'form', :locals => { :f => f } %>
	<p>
		<%= submit_tag l(:button_create) %>
	</p>
<% end %>
