<% content_for :config_sidebar do %>
  <%= render :partial => 'configurations/config_sidebar' %>
<% end %>

<h2><%= link_to l(:field_controller_set) + "s", project_configuration_controller_sets_path(@project),:class => 'config-header' %> &#187; <%= l(:controller_sets_new) %></h2>

<% labelled_tabular_form_for :controller_set, @cset, :url => { :action => "create" } do |f| %>
	<%= render :partial => 'form', :locals => { :f => f } %>
	<p>
		<%= submit_tag l(:button_create) %>
	</p>
<% end %>
