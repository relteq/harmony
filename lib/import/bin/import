#!/usr/bin/env ruby
require 'lib/import/import-files/init'


xml_file = ARGV[0]
project_id = ARGV[1]
db_url = ARGV[2]
is_new = ARGV[3] || false
am_testing = ARGV[4] || false

DB = ActiveRecord::Base.establish_connection(
 :adapter => "sqlite3",
 :database  => Dir.pwd + "/db/development.sqlite3")


sc_id = Aurora.import(open(xml_file),project_id, {:is_new => is_new , :am_testing => am_testing})

p xml_file
